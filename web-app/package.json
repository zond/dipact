  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --maxWorkers=50% --isolatedModules=true",
    "test-coverage": "test --silent --ci --coverage --coverageReporters=\"text\" --coverageReporters=\"text-summary\"",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "storybook-axe": "build-storybook && axe-storybook",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "lint-fix": "eslint src/**/*.{js,ts,tsx} --fix"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
