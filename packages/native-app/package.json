{
  "name": "diplicitynative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "rimraf node_modules",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test-coverage": "jest --silent --detectOpenHandles --ci --coverage",
    "test-coverage-ci": "jest --silent --detectOpenHandles --ci --coverage --coverageReporters=\"text\" --coverageReporters=\"text-summary\"",
    "lint": "eslint src/**/*.{ts,tsx} --max-warnings 0",
    "build-and-install-common": "cd ../common && yarn build && cd ../native-app && yarn add ../common"
  },
  "dependencies": {
    "@diplicity/common": "file:../common",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/cli-platform-android": "^5.0.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-google-signin/google-signin": "^7.1.0",
    "@react-native-picker/picker": "^2.4.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/drawer": "^6.1.4",
    "@react-navigation/material-top-tabs": "^6.5.2",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.6.1",
    "@react-navigation/stack": "^6.0.7",
    "@reduxjs/toolkit": "^1.7.2",
    "@rneui/base": "^4.0.0-rc.1",
    "@rneui/themed": "^4.0.0-rc.1",
    "axios": "^1.4.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "contrast-color": "^1.0.1",
    "formik": "^2.2.9",
    "i18next": "^21.6.11",
    "jquery": "^3.6.0",
    "react": "^18.2.0",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-i18next": "^11.15.4",
    "react-native": "0.64.1",
    "react-native-config": "^1.4.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-pan-zoom": "^2.1.12",
    "react-native-pager-view": "^6.1.2",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.5.0",
    "react-native-svg": "^12.3.0",
    "react-native-svg-uri": "^1.2.3",
    "react-native-tab-view": "^3.3.4",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^7.2.6",
    "xmldom": "^0.6.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "29.5.2",
    "@types/node-fetch": "^2.6.2",
    "@types/react-native": "^0.64.5",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/xml2js": "^0.4.11",
    "@types/xmldom": "^0.1.31",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/setup.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.tsx"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*test).tsx?$",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native(-.*)?|@rneui?|@react-navigation|@react-native(-community)?)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/react-app-env.d.ts",
      "<rootDir>/src/mocks",
      "<rootDir>/src/App.tsx",
      "<rootDir>/src/screens/Router.tsx"
    ]
  }
}
